{
  "short_name": "React App",
  "name": "Create React App Sample",
  "icons": [
    {
      "src": "logo128.png",
      "type": "image/png",
      "sizes": "128x128"
    },
    {
      "manifest_version": 3,
      "name": "WooniPangi",
      "version": "1.0",
      "description": "Let's meet Wooni and Pang-i!",
      "action": {
        "default_popup": "index.html",
        "default_icon": {
          "16": "logo16.png",
          "48": "logo48.png",
          "128": "logo128.png"
        }
      },
      "permissions": [
        "activeTab"
      ],
      "background": {
        "service_worker": "background.js"
      }
    }
  ],
  "start_url": ".",
  "display": "standalone",
  "theme_color": "#000000",
  "background_color": "#ffffff"
}
